        <!-- virtualgenericid -->
    <nodeDef id="virtualgeneric" nls="vgeneric">
      <editors />
      <sts>
        <st id="ST" editor="value" />
        <st id="OL" editor="value" />
        <st id="GV0" editor="ONLEVELTYPE" />
      </sts>
      <cmds>
		<sends>
		  <cmd id="DON" />
		  <cmd id="DOF" />
		  <cmd id="DFON" />
		  <cmd id="DFOF" />
		  <cmd id="BRT" />
		  <cmd id="DIM" />
		  <cmd id="ST" />
		  <cmd id="SETST" />
		  <cmd id="SETOL" />
		  <cmd id="OLTT" />
        </sends>
        <accepts>
          <cmd id="DON" />
          <cmd id="DOF" />
		  <cmd id="DFON" />
		  <cmd id="DFOF" />
          <cmd id="BRT" />
          <cmd id="DIM" />
		  <cmd id="SETST">
			<p id="" editor="value" init="ST" />
          </cmd>
		  <cmd id="SETOL">
			<p id="" editor="value" init="OL" />
		  </cmd>
		  <cmd id="OLTT" />
        </accepts>
      </cmds>
    </nodeDef>
        <!-- virtualgarageid -->
    <nodeDef id="virtualgarage" nls="GARAGE">
        <editors />
        <sts>
          <st id="GV0" editor="GLIGHT" />
          <st id="GV1" editor="GDOOR" />
          <st id="GV2" editor="GCOMMAND" />
          <st id="GV8" editor="GMOTOR" />
          <st id="GV9" editor="GDOOR" />
          <st id="GV3" editor="GMOTION" />
          <st id="GV4" editor="GLOCK" />
          <st id="GV5" editor="GOBST" />  
          <st id="GV6" editor="minutes" />
          <st id="GV7" editor="seconds" />
        </sts>
        <cmds>
          <sends>
            <cmd id="DON" />
            <cmd id="DOF" />
			<cmd id="LT_ON" />
			<cmd id="LT_OFF" />
            <cmd id="OPEN" />
            <cmd id="CLOSE" />
            <cmd id="TRIGGER" />
            <cmd id="STOP" />
            <cmd id="LOCK" />
			<cmd id="UNLOCK" />
            <cmd id="MOTORON" />
			<cmd id="MOTOROFF" />
			<cmd id="MOTION" />
			<cmd id="NOMOTION" />
			<cmd id="OBSTRUCTION" />
			<cmd id="NOOBSTRUCTION" />
            <cmd id="resetStats" />
          </sends>
          <accepts>
            <cmd id="LT_ON" />
            <cmd id="LT_OFF" />
            <cmd id="OPEN" />
            <cmd id="CLOSE" />
            <cmd id="TRIGGER" />
            <cmd id="STOP" />
            <cmd id="LOCK" />
			<cmd id="UNLOCK" />
            <cmd id="QUERY" />
            <cmd id="resetStats" />
          </accepts>
        </cmds>
    </nodeDef>
</nodeDefs>
