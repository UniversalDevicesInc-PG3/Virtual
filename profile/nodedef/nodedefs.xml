<nodeDefs>
    <!-- NODE DEF from 5.0 document http://wiki.universal-devices.com/index.php?title=V50_MAIN -->
    <!-- controller, this must mach the id in your Controller class-->
    <nodeDef id="controller" nls="ctl">
        <editors />
        <sts>
	    <st id="ST" editor="bool" />
	    </sts>
        <cmds>
            <sends />
            <accepts>
              <cmd id="QUERY" />
              <cmd id="DISCOVER" />
              <cmd id="REMOVE_NOTICES_ALL" />
              <cmd id="REMOVE_NOTICE_TEST" />
              <cmd id="UPDATE_PROFILE" />
            </accepts>
        </cmds>
    </nodeDef>
	
	
	<!-- virtualswitchid -->
    <nodeDef id="virtualswitch" nls="vswitch">
        <editors />
        <sts>
            <st id="ST" editor="on_off" />
        </sts>
        <cmds>
            <sends />
            <accepts>
                <cmd id="DON" />
                <cmd id="DOF" />		    
            </accepts>
        </cmds>
    </nodeDef>
	
	
	<!-- virtualtempid -->
    <nodeDef id="virtualtemp" nls="vtemp">
        <editors />
        <sts>
            <st id="ST" editor="temp" />
            <st id="GV1" editor="temp" />
        </sts>
        <cmds>
            <sends />
            <accepts>
		<cmd id="setTemp">
	          <p id="" editor="temp" init="ST" />
		    </cmd>
            </accepts>
        </cmds>
    </nodeDef>
	
	
        <!-- virtualtempcid -->
    <nodeDef id="virtualtempcelsius" nls="vtempcelsius">
        <editors />
        <sts>
            <st id="ST" editor="tempc" />
            <st id="GV1" editors="tempc" />
        </sts>
        <cmds>
            <sends />
            <accepts>
                <cmd id="setRaw" />
                <cmd id="setFtoC" />
                <cmd id="setTemp">
                  <p id="" editor="tempc" init="ST" />
                    </cmd>
            </accepts>
        </cmds>
    </nodeDef>	
	
	
	<!-- virtualdimmerid -->
    <nodeDef id="virtualdimmer" nls="vdimmer">
        <editors />
        <sts>
            <st id="ST" editor="level" />
        </sts>
        <cmds>
            <sends />
            <accepts>
                <cmd id="DON" />
                <cmd id="DOF" />
		<cmd id="setDim">
	          <p id="" editor="level" init="ST" />
		    </cmd>		    
            </accepts>
        </cmds>
    </nodeDef>
	
	
        <!-- virtualgenericid -->
    <nodeDef id="virtualgeneric" nls="vgeneric">
        <editors />
        <sts>
            <st id="ST" editor="level" />
        </sts>
        <cmds>
            <sends />
            <accepts>
                <cmd id="DON" />
                <cmd id="DOF" />
                <cmd id="setDim">
                  <p id="" editor="level" init="ST" />
                    </cmd>
            </accepts>
        </cmds>
    </nodeDef>
	
</nodeDefs>
