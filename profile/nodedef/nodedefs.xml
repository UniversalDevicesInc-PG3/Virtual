<nodeDefs>
    <!-- NODE DEF from 5.0 document http://wiki.universal-devices.com/index.php?title=V50_MAIN -->
    <!-- controller, this must mach the id in your Controller class-->
    <nodeDef id="controller" nls="ctl">
        <editors />
        <sts>
            <st id="ST" editor="CTD" />
            <st id="GV0" editor="ID" />
        </sts>
        <cmds>
          <sends>
            <!-- These are for our heartbeat -->
            <cmd id="DON" />
            <cmd id="DOF" />
          </sends>
          <accepts>
            <cmd id="QUERY" />
            <cmd id="DISCOVER" />
          </accepts>
        </cmds>
    </nodeDef>
        <!-- virtualswitchid -->
    <nodeDef id="virtualswitch" nls="vswitch">
        <editors />
        <sts>
          <st id="ST" editor="on_off" />
        </sts>
        <cmds>
          <sends>
            <cmd id="DON" />
            <cmd id="DOF" />
          </sends>
          <accepts>
            <cmd id="DON" />
            <cmd id="DOF" />
            <cmd id="TOGGLE" />
          </accepts>
        </cmds>
    </nodeDef>
		<!-- virtualononlyid -->
	<nodeDef id="virtualononly" nls="vononly">
		<editors />
		<sts>
		  <st id="ST" editor="on_off" />
		</sts>
		<cmds>
		  <sends>
			<cmd id="DON" />
		  </sends>
		  <accepts>
			<cmd id="DON" />
			<cmd id="DOF" />
		  </accepts>
		</cmds>
	</nodeDef>
		<!-- virtualondelayid -->
	<nodeDef id="virtualondelay" nls="vondelay">
		<editors />
		<sts>
		  <st id="ST" editor="on_off_delay" />
		  <st id="DUR" editor="delay" />
		</sts>
		<cmds>
		  <sends>
			<cmd id="DON" />
			<cmd id="DOF" />
			<cmd id="TIMER" />
		  </sends>
		  <accepts>
			<cmd id="DON" />
			<cmd id="DFOF" />
			<cmd id="DOF" />
			<cmd id="SETDELAY">
			  <p id="" editor="delay" init="DUR" />
			</cmd>
		  </accepts>
		</cmds>
	</nodeDef>
		<!-- virtualoffdelayid -->
	<nodeDef id="virtualoffdelay" nls="voffdelay">
		<editors />
		<sts>
		  <st id="ST" editor="on_off_delay" />
		  <st id="DUR" editor="delay" />
		</sts>
		<cmds>
		  <sends>
			<cmd id="DON" />
			<cmd id="DOF" />
			<cmd id="TIMER" />
		  </sends>
		  <accepts>
			<cmd id="DON" />
			<cmd id="DOF" />
			<cmd id="SETDELAY">
			  <p id="" editor="delay" init="DUR" />
			</cmd>
		  </accepts>
		</cmds>
	</nodeDef>
		<!-- virtualtoggleid -->
	<nodeDef id="virtualtoggle" nls="vtoggle">
		<editors />
		<sts>
		  <st id="ST" editor="on_off_toggle" />
		  <st id="DUR" editor="delay" />
		  <st id="GV0" editor="delay" />
		</sts>
		<cmds>
		  <sends>
			<cmd id="DON" />
			<cmd id="DOF" />
			<cmd id="ONTIMER" />
			<cmd id="OFFTIMER" />
		  </sends>
		  <accepts>
			<cmd id="DON" />
			<cmd id="DOF" />
			<cmd id="DFON" />
			<cmd id="DFOF" />
			<cmd id="SETONDUR">
			  <p id="" editor="delay" init="DUR" />
			</cmd>
			<cmd id="SETOFFDUR">
			  <p id="" editor="delay" init="GV0" />
			</cmd>
		  </accepts>
		</cmds>
	</nodeDef>
        <!-- virtualtempid -->
    <nodeDef id="virtualtemp" nls="vtemp">
        <editors />
        <sts>
          <st id="ST" editor="temp" />
          <st id="GV1" editor="temp" />
          <st id="GV2" editor="minutes" />
          <st id="GV3" editor="temp" />
          <st id="GV4" editor="temp" />  
          <st id="GV5" editor="temp" />
          <st id="GV6" editor="action_type" />
          <st id="GV7" editor="variable_type" />
          <st id="GV8" editor="variableID" />
          <st id="GV9" editor="action_type" />  
          <st id="GV10" editor="variable_type" />
          <st id="GV11" editor="variableID" />
          <st id="GV12" editor="on_off" />  
          <st id="GV13" editor="on_off" />
        </sts>
        <cmds>
          <sends />
          <accepts>
            <cmd id="resetStats" />
            <cmd id="setTemp">
              <p id="" editor="temp" init="ST" />
            </cmd>
            <cmd id="setAction1">
              <p id="" editor="action_type" init="GV6" />
            </cmd>
            <cmd id="setAction1type">
              <p id="" editor="variable_type" init="GV7" />
            </cmd>
            <cmd id="setAction1id">
              <p id="" editor="variableID" init="GV8" />
            </cmd> 
            <cmd id="setAction2">
              <p id="" editor="action_type" init="GV9" />
            </cmd>
            <cmd id="setAction2type">
              <p id="" editor="variable_type" init="GV10" />
            </cmd>
            <cmd id="setAction2id">
              <p id="" editor="variableID" init="GV11" />
            </cmd>
            <cmd id="setRawToPrec">
              <p id="" editor="on_off" init="GV12"/>
            </cmd>                
            <cmd id="setCtoF">
              <p id="" editor="on_off" init="GV13"/>
            </cmd>                
          </accepts>
        </cmds>
    </nodeDef>
