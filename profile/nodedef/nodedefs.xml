        <!-- virtualgenericid -->
    <nodeDef id="virtualgeneric" nls="vgeneric">
        <editors />
        <sts>
            <st id="ST" editor="value" />
        </sts>
        <cmds>
            <sends />
            <accepts>
                <cmd id="DON" />
                <cmd id="DOF" />
                <cmd id="setDim">
                  <p id="" editor="value" init="ST" />
                    </cmd>
            </accepts>
        </cmds>
    </nodeDef>
