
<editors>
    <!-- Boolean -->
    <editor id="bool">
        <range uom="2" subset="0,1" />
    </editor>

    <editor id="on_off">
        <range uom="25" subset="0,1" nls="ON_OFF" />
    </editor>
    
    <editor id="action_type">
        <range uom="25" subset="0-2" nls="ACTION" />
    </editor>
    
    <editor id="variable_type">
        <range uom="25" subset="0-4" nls="TYPE" />
    </editor>    
    
    <editor id="value">
        <range uom="56" min="0" max="100" prec="0" step="1"/>
    </editor>

    <editor id="variableID">
        <range uom="56" min="0" max="1000" prec="0" step="1"/>
    </editor>
    
    <editor id="minutes">
        <range uom="45" min="0" max="9999" prec="2" step=".5"/>
    </editor>
    
    <editor id="seconds">
        <range uom="58" min="0" max="9999" prec="0" step="1"/>
    </editor>
    
    <editor id="level">
        <range uom="56" min="0" max="100" prec="0" step="1"/>
    </editor>

    <editor id="temp">
        <range uom="17" min="-30" max="130" step="1" prec="1"/>
    </editor>

     <editor id="tempc">
        <range uom="4" min="-60" max="130" step="1" prec="1"/>
    </editor>

    <editor id="GLIGHT">
        <range uom="2" subset="0,1" nls="GLIGHT" />
    </editor>
    <editor id="GDOOR">
        <range uom="25" subset="0,1,2,3,4" nls="GDOOR" />
    </editor>
    <editor id="GCOMMAND">
        <range uom="25" subset="0,1,2,3,4" nls="GCOMMAND" />
    </editor>
    <editor id="GMOTOR">
        <range uom="2" subset="0,1" nls="GMOTOR" />
    </editor>
    <editor id="GMOTION">
        <range uom="2" subset="0,1" nls="GMOTION" />
    </editor>
    <editor id="GLOCK">
        <range uom="2" subset="0,1" nls="GLOCK" />
    </editor>
    <editor id="GOBST">
        <range uom="2" subset="0,1" nls="GOBST" />
    </editor>

</editors>
